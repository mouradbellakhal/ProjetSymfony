{% extends 'base.html.twig' %}

{% block header %}<h1>Users</h1>{% endblock %}

{% block body %}
<ul>
    {% for user in users %}
        <li><strong>{{ user.fullname }}</strong> - {{ user.username }} ({{ user.email }})</li>
    {% else %}
        <li>No users.</li>
    {% endfor %}
</ul>
{% endblock %}
